  <!-- Telegram section -->
  <section class="content-section vertical-center" id="telegram">
    <div class="container text-center my-auto style="height: 100vh;"">
      <div class="row">
        <div class="col-lg-24 mx-auto">
          <h2>Orçamentos, dúvidas e dicas da Mila</h2>
          <p class="lead mb-5">Contamos com sua mensagem!</p>
          <p><br><br> </p>
          <div id="form-contatct" class="text-left col-lg-24">
            <form id="pesquisa" action="/contact" method="post">
              <!-- name form -->
              <div class="form-group">
                <label for="nameForm">Nome</label>
                <input type="text" class="form-control" id="nameForm" name="nameForm" placeholder="Nome completo"
                  required>
              </div>
              <!-- email form -->
              <div class="form-group">
                <label for="emailForm">Endereço de email (opcional)</label>
                <input type="email" class="form-control" id="emailForm" name="emailForm" placeholder="nome@email.com">
              </div>
              <!-- phone form -->
              <div class="form-group">
                <label for="phoneForm">Telefone</label>
                <input type="phone" class="form-control" id="phoneForm" name="phoneForm" placeholder="(48)91122-3344"
                  required>
              </div>
              <!-- message text -->
              <div class="form-group">
                <label for="messageTx">Messagem</label>
                <textarea class="form-control" id="messageTx" name="messageTx" rows="3" placeholder="Orçamentos, dúvidas, críticas, sugestões..."
                  required></textarea>
              </div>
              <div class="form-group">
                <button type="submit" class="btn btn-dark">Enviar</button>
              </div>


              <% var msgLength = messageSuccess.length %>

              <% if ((messageSuccess.length > 0)) { %>
              <div class="form-group alert alert-success  alert-dismissible" role="alert" id=success-message>
                <%= messageSuccess %>
              </div>
              <% } %>
              <% if ((messageError.length > 0)) { %>
              <div class="form-group alert alert-danger alert-dismissible " role="alert" id=success-message>
                <%= messageError %>
              </div>
              <% } %>

            </form>
          </div>
        </div>
      </div>
    </div>
  </section>

